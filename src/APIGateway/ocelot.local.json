{
  "Routes": [
    {
      "UpstreamPathTemplate": "/user-api/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "PATCH", "DELETE" ],
      "DownstreamPathTemplate": "/user-api/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7075
        }
      ],
      "SwaggerKey": "user",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtAuth"
      }
    },
    {
      "UpstreamPathTemplate": "/claim-service/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "PATCH", "DELETE" ],
      "DownstreamPathTemplate": "/claim-service/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7077
        }
      ],
      "SwaggerKey": "claims",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtAuth"
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "user",
      "Config": [
        {
          "Name": "Users API",
          "Version": "v1",
          "Url": "https://localhost:7075/user-api/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "claims",
      "Config": [
        {
          "Name": "Claims API",
          "Version": "v1",
          "Url": "https://localhost:7077/claim-service/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7195"
  }
}
